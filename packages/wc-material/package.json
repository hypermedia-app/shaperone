{
  "name": "@hydrofoil/shaperone-wc-material",
  "version": "0.2.1",
  "description": "Rendering forms in Material Design style",
  "main": "index.js",
  "type": "module",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "prepack": "tsc",
    "analyze": "run-p analyze:*",
    "analyze:wc": "wca --format json --outFile ./custom-elements.json",
    "analyze:readme": "wca --outFile ./README.md",
    "analyze:lit": "lit-analyzer . --strict --quiet --rules.no-invalid-css off"
  },
  "dependencies": {
    "@hydrofoil/shaperone-core": "0.6.5",
    "@hydrofoil/shaperone-wc": "0.4.1",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-list": "^0.20.0",
    "@material/mwc-menu": "^0.20.0",
    "@material/mwc-tab": "^0.20.0",
    "@material/mwc-tab-bar": "^0.20.0",
    "@material/mwc-select": "^0.20.0",
    "@material/mwc-textarea": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@open-wc/lit-helpers": "^0.3.10",
    "@rdf-esm/data-model": "^0.5.3",
    "@tpluscode/rdf-ns-builders": "^0.4.0",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "wc-menu-button": "^3.1"
  },
  "repository": "https://github.com/hypermedia-app/shaperone",
  "author": "Tomasz Pluskiewicz <awesome@hypermedia.app>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@open-wc/eslint-config": "^2.0.6",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3",
    "@rdfine/shacl": "^0.7.3",
    "@rdf-esm/dataset": "^0.5.0",
    "@rdf-esm/namespace": "^0.5.1",
    "@shaperone/testing": "*",
    "clownface": "^1",
    "deepmerge": "^4.2.2",
    "eslint-plugin-lit": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
