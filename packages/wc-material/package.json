{
  "name": "@hydrofoil/shaperone-wc-material",
  "version": "0.6.2",
  "description": "Rendering forms in Material Design style",
  "main": "index.js",
  "type": "module",
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "custom-elements.json"
  ],
  "scripts": {
    "prepack": "tsc",
    "analyze": "run-p analyze:*",
    "analyze:wc": "wca --format json --outFile ./custom-elements.json",
    "analyze:readme": "wca --outFile ./README.md"
  },
  "dependencies": {
    "@hydrofoil/shaperone-core": "^0.13.0",
    "@hydrofoil/shaperone-wc": "^0.9.0",
    "@material/mwc-icon": "^0.25",
    "@material/mwc-list": "^0.25",
    "@material/mwc-menu": "^0.25",
    "@material/mwc-tab": "^0.25",
    "@material/mwc-tab-bar": "^0.25",
    "@material/mwc-select": "^0.25",
    "@material/mwc-textarea": "^0.25",
    "@material/mwc-textfield": "^0.25",
    "@open-wc/lit-helpers": "^0.7.0",
    "@rdfjs-elements/lit-helpers": "^0.3.7",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "lit": "^3",
    "wc-menu-button": "^3.1"
  },
  "repository": "https://github.com/hypermedia-app/shaperone",
  "author": "Tomasz Pluskiewicz <awesome@hypermedia.app>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@open-wc/testing": "^4.0.0",
    "@rdfine/shacl": "^0.10.5",
    "@zazuko/env": "^2.1.0",
    "@shaperone/testing": "*",
    "deepmerge": "^4.2.2",
    "eslint-plugin-lit": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
