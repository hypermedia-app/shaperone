PREFIX ex: <http://example.org/>
PREFIX dash: <http://datashapes.org/dash#>
PREFIX sh: <http://www.w3.org/ns/shacl#>

<>
  a sh:NodeShape ;
  sh:property
    [
      sh:name "text field" ;
      sh:path ex:textField ;
      dash:editor dash:TextFieldEditor ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ],
    [
      sh:name "text area" ;
      sh:path ex:textArea ;
      dash:editor dash:TextAreaEditor ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ],
    [
      sh:name "text field with lang" ;
      sh:path ex:textFieldWithLang ;
      dash:editor dash:TextFieldWithLangEditor ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ],
    [
      sh:name "text area with lang" ;
      sh:path ex:textAreaWithLang ;
      dash:editor dash:TextAreaWithLangEditor ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ];
.
