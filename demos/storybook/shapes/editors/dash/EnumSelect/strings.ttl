PREFIX dash: <http://datashapes.org/dash#>
PREFIX schema: <http://schema.org/>
PREFIX sh: <http://www.w3.org/ns/shacl#>

<>
  a sh:NodeShape ;
  sh:property
    [
      sh:name "Category" ;
      sh:path schema:category ;
      dash:editor dash:EnumSelectEditor ;
      sh:in ( "A" "B" "C" ) ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ]
.
